#: mastodon2memos/commands/__init__.py:16
#: mastodon2memos/commands/commands.py:16
#, python-brace-format
msgid ""
"Configuration file not found at {config_path}. Please ensure the file exists."
msgstr "{config_path} に設定ファイルが見つかりません。ファイルが存在することを確認してください。"

#: mastodon2memos/commands/__init__.py:29
#: mastodon2memos/commands/commands.py:29
#, python-brace-format
msgid "Missing section \"{section}\" in configuration file."
msgstr "設定ファイルに \"{section}\" セクションがありません。"

#: mastodon2memos/commands/__init__.py:33
#: mastodon2memos/commands/commands.py:33
#, python-brace-format
msgid "Missing key \"{key}\" in section \"{section}\" of configuration file."
msgstr "設定ファイルの \"{section}\" セクションに \"{key}\" キーがありません。"

#: mastodon2memos/commands/__init__.py:60
#: mastodon2memos/commands/commands.py:60
msgid "Successfully connected to Mastodon API."
msgstr "Mastodon API に正常に接続されました。"

#: mastodon2memos/commands/__init__.py:62
#: mastodon2memos/commands/commands.py:62
msgid "Failed to connect to Mastodon API."
msgstr "Mastodon API に接続できませんでした。"

#: mastodon2memos/commands/__init__.py:69
#: mastodon2memos/commands/commands.py:69
msgid "Successfully connected to Memos API."
msgstr "Memos API に正常に接続されました。"

#: mastodon2memos/commands/__init__.py:71
#: mastodon2memos/commands/commands.py:71
msgid "Failed to connect to Memos API."
msgstr "Memos API に接続できませんでした。"

#: mastodon2memos/commands/mastodon2memos.py:6
msgid "Publish Mastodon toots as Memos."
msgstr "Mastodon トゥートを Memos として公開します。"

#: mastodon2memos/commands/mastodon2memos.py:7
msgid "Ask before creating a new memo from a toot."
msgstr "トゥートから新しいメモを作成する前に確認します。"

#: mastodon2memos/commands/mastodon2memos.py:8
#: mastodon2memos/commands/troubleshoot.py:7
msgid "Path to the configuration file."
msgstr "設定ファイルのパス。"

#: mastodon2memos/commands/mastodon2memos.py:25
#, python-brace-format
msgid "Processing toot: {url}"
msgstr "トゥートを処理中: {url}"

#: mastodon2memos/commands/mastodon2memos.py:28
#, python-brace-format
msgid ""
"Do you want to attempt publishing this toot as a memo?\n"
"{content}\n"
"{url}?"
msgstr ""
"このトゥートをメモとして公開しますか？\n"
"{content}\n"
"{url}?"

#: mastodon2memos/commands/mastodon2memos.py:29
msgid "Skipping toot."
msgstr "トゥートをスキップします。"

#: mastodon2memos/commands/mastodon2memos.py:33
msgid "New toot added to memos."
msgstr "新しいトゥートがメモに追加されました。"

#: mastodon2memos/commands/mastodon2memos.py:35
msgid "No memo added, toot was already saved in memos."
msgstr "メモが追加されていません。トゥートは既にメモに保存されています。"

#: mastodon2memos/commands/mastodon2memos.py:37
msgid "Finished processing toots."
msgstr "トゥートの処理が完了しました。"

#: mastodon2memos/commands/troubleshoot.py:6
msgid "Ensure proper connection to both Mastodon and Memos APIs."
msgstr "Mastodon と Memos API に正しく接続されていることを確認してください。"

#: mastodon2memos/commands/troubleshoot.py:20
#, python-brace-format
msgid "Successfully resolved Mastodon username to user ID: {user_id}"
msgstr "Mastodon ユーザー名をユーザー ID に正常に解決しました: {user_id}"

#: mastodon2memos/commands/troubleshoot.py:22
msgid "Failed to resolve Mastodon username to user ID."
msgstr "Mastodon ユーザー名をユーザー ID に解決できませんでした。"

#: mastodon2memos/clients/mastodon.py:51
#, python-brace-format
msgid "Failed to fetch latest public toots: {error}"
msgstr "最新の公開トゥートを取得できませんでした: {error}"

#: mastodon2memos/clients/memos.py:42
#, fuzzy, python-brace-format
msgid "> 🌐 [original toot]({url}) posted by [{acct}]({account_url}) #{tag}"
msgstr "> 🌐 [{acct}]({account_url}) によって投稿された [元のトゥート]({url}) #{tag}"
